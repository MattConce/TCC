{
  "name": "eye-tracker",
  "version": "1.0.0",
  "description": "Low cost webcam based eye tracker",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec babel-node backend/server.js",
    "build": "rm -rf dist && babel backend -d dist",
    "heroku-postbuild": "npm run build && mkdir uploads/ && cd frontend && npm install && npm run build"
  },
  "author": "mattconce",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.13",
    "multer": "^1.4.2",
    "react-countdown-circle-timer": "^2.5.3",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.3"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
